# Cloudflare Worker Environment Variables
# Copy this file to .env and fill in your values

# Cloudflare Account ID (required for deployment)
CLOUDFLARE_ACCOUNT_ID=your-account-id-here

# Foreman Orchestrator Configuration
FOREMAN_URL=https://your-foreman-instance.com
FOREMAN_API_KEY=your-foreman-api-key-here

# Optional: OpenAI API for enhanced reasoning
# OPENAI_API_KEY=sk-your-openai-key-here

# Agent Behavior Tuning
REFLEX_THRESHOLD=0.7
SYNC_INTERVAL_SECONDS=5
MISSION_CACHE_TTL=300

# Minecraft Server Coordinates (for reference)
SPAWN_X=0
SPAWN_Y=64
SPAWN_Z=0

# Logging
LOG_LEVEL=info  # debug, info, warn, error
